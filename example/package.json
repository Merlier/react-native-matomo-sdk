{
  "name": "react-native-matomo-sdk-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "postinstall": "install-local",
    "test": "npm run test:js && npm run test:android && npm run test:ios",
    "test:js": "jest",
    "test:android": "cd android/ && ./gradlew app:build",
    "test:ios": "cd ios/ && xcodebuild -project example.xcodeproj -configuration Debug -scheme example -quiet CODE_SIGN_IDENTITY='' CODE_SIGNING_REQUIRED=NO"
  },
  "dependencies": {
    "react": "16.6.3",
    "react-native": "0.58.4"
  },
  "localDependencies": {
    "react-native-matomo-sdk": "../"
  },
  "devDependencies": {
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "24.1.0",
    "install-local": "^1.0.0",
    "jest": "24.1.0",
    "metro-react-native-babel-preset": "0.51.1",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
